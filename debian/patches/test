+    u''     #  0x008c -> LATIN SMALL LETTER I WITH CIRCUMFLEX
+    u''     #  0x008d -> LATIN SMALL LETTER I WITH GRAVE
+    u''     #  0x008e -> LATIN CAPITAL LETTER A WITH DIAERESIS
+    u''     #  0x008f -> LATIN CAPITAL LETTER A WITH RING ABOVE
